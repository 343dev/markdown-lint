{
  "name": "@funboxteam/markdown-lint",
  "version": "1.1.1",
  "description": "Markdown code style linter.",
  "main": "index.js",
  "bin": {
    "markdown-lint": "./cli.js"
  },
  "scripts": {
    "lint": "eslint --cache --ext .js ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "markdown",
    "linter"
  ],
  "devDependencies": {
    "@funboxteam/eslint-config": "4.14.0",
    "eslint": "5.16.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11"
  },
  "dependencies": {
    "commander": "5.1.0",
    "eyo-kernel": "2.5.6",
    "glob": "7.1.6",
    "prettier": "1.15.3",
    "remark": "10.0.1",
    "remark-preset-lint-markdown-style-guide": "2.1.2",
    "remark-textr": "3.0.3",
    "typograf": "6.11.1",
    "vfile-reporter": "6.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --cache"
    ]
  }
}
